#integer = whole number Float = decimal
print(2/7)
universe_age = 14_000_000_000
print(universe_age)
print(2+3)
print(2-3)
print(2*3)
print(2/3)
#epxonents
print(2**3)
print(3**3)
print(10**6)
#order of operations
print(2+3*4)
print((2+3) * 4)
#floats
print(0.1 + 0.1)
print(2 * 0.1)
print(2 * 0.2)
print(4/2)
print(1 + 2.0)
print(2 * 3.0)
print(3.0 ** 2)
